{
    "name": "camagru/web-app",
    "description": "Camagru web application - A photo sharing and editing platform",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-pgsql": "*",
        "phpmailer/phpmailer": "^6.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
